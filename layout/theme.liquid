    <script>window.jQuery || document.write('<script src="{{ 'jquery-1.7.1.min.js' | asset_url }}"><\/script>')</script>
    <script src="{{ 'bootstrap.js' | asset_url }}"></script>
    <script src="{{ '_bootstrapify.js' | asset_url }}"></script>

