    <div class="navbar navbar-fixed-top">
      <div class="navbar-inner">
        <div class="container"> 
          
          <div class="pull-left">
            <form class="bootstrapify-search pull-left">
              <input type="search" placeholder="Search..." x-webkit-speech="">
            </form>
          </div>
          
          <a class="brand hidden" href="/">{{ shop.name }}</a>
          
          <div class="pull-right">
            {% if shop.customer_accounts_enabled %}
              <div class="pull-left">
                <a class="btn btn-inverse" href="/account"><i class="icon-user icon-white"></i> Login</a>
              </div>
              <div class="divider-vertical pull-left"></div>
            {% endif %}
            <div class="btn-group pull-left">
              <a class="btn btn-inverse" href="/cart"><i class="icon-shopping-cart icon-white"></i> Cart ({{ cart.item_count }})</a>
              <a class="btn btn-inverse dropdown-toggle" data-toggle="dropdown" href="#">
                <span class="caret"></span>
              </a>
              <ul class="dropdown-menu">
                {% if cart.item_count == 0 %}
                  <li><a href="#">There are no items in your cart.</a></li>
                {% else %}
                  <li><a href="#">1 x Item 1</a></li>
                  <li><a href="#">1 x Item 2</a></li>
                  <li><a href="#">2 x Item 3</a></li>
                  <li class="divider"></li>
                  <li><a href="/checkout"><i class="icon-share-alt"></i> Checkout</a></li>
                {% endif %}
              </ul>
            </div>
            <div class="divider-vertical pull-left"></div>
            <a class="btn btn-success pull-left" href="{% if cart.item_count == 0 %}/cart{% else %}/checkout{% endif %}">Checkout</a>
          </div>
          
        </div>
      </div>
    </div>
